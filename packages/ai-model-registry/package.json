{
    "name": "@anolilab/ai-model-registry",
    "version": "1.3.0",
    "description": "Unified registry for AI model providers and their metadata",
    "keywords": [
        "ai",
        "artificial-intelligence",
        "llm",
        "large-language-models",
        "models",
        "model-registry",
        "providers",
        "registry",
        "metadata",
        "openai",
        "anthropic",
        "google",
        "meta",
        "groq",
        "deepseek",
        "mistral",
        "pricing",
        "cost",
        "search",
        "filter",
        "typescript",
        "javascript",
        "node",
        "browser"
    ],
    "homepage": "https://github.com/anolilab/ai-models",
    "bugs": {
        "url": "https://github.com/anolilab/ai-models/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/anolilab/ai-models.git",
        "directory": "packages/ai-model-registry"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "license": "Apache-2.0",
    "author": "AnoliLab",
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./schema": {
            "types": "./dist/schema.d.ts",
            "import": "./dist/schema.js"
        },
        "./icons": {
            "types": "./dist/icons-sprite.d.ts",
            "import": "./dist/icons-sprite.js"
        },
        "./api.json": "./public/api.json"
    },
    "files": [
        "./dist",
        "./public",
        "README.md",
        "CHANGELOG.md",
        "LICENSE.md"
    ],
    "scripts": {
        "aggregate": "tsx ./scripts/aggregate-providers.ts",
        "build": "packem build --development",
        "build:prod": "packem build --production",
        "clean": "rimraf node_modules dist .eslintcache",
        "cleanup-icons": "tsx ./scripts/cleanup-duplicate-icons.ts",
        "download": "tsx ./scripts/download/index.ts",
        "download:fast": "tsx ./scripts/download/index.ts --concurrency 10",
        "download:slow": "tsx ./scripts/download/index.ts --concurrency 2",
        "download:max": "tsx ./scripts/download/index.ts --concurrency 20",
        "generate-icons": "tsx ./scripts/generate-svg-sprite.ts",
        "install:chrome": "npx puppeteer browsers install chrome",
        "lint:attw": "attw --pack",
        "lint:eslint": "eslint .",
        "lint:eslint:fix": "eslint . --fix",
        "lint:prettier": "prettier --check .",
        "lint:prettier:fix": "prettier --write .",
        "lint:types": "tsc --noEmit",
        "prepare-data": "pnpm run download && pnpm run aggregate && pnpm run generate-icons",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui --coverage.enabled=true",
        "test:watch": "vitest"
    },
    "dependencies": {
        "zod": "^4.0.17"
    },
    "devDependencies": {
        "@anolilab/eslint-config": "^16.2.26",
        "@anolilab/prettier-config": "^6.0.8",
        "@anolilab/semantic-release-clean-package-json": "3.0.5",
        "@anolilab/semantic-release-pnpm": "^2.0.5",
        "@arethetypeswrong/cli": "^0.18.2",
        "@lobehub/icons-static-svg": "^1.63.0",
        "@secretlint/secretlint-rule-preset-recommend": "^11.0.2",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/commit-analyzer": "13.0.1",
        "@semantic-release/exec": "^7.1.0",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/github": "^11.0.4",
        "@semantic-release/release-notes-generator": "14.0.3",
        "@total-typescript/ts-reset": "^0.6.1",
        "@types/node": "^24.2.1",
        "@visulima/colorize": "1.4.23",
        "@visulima/packem": "2.0.0-alpha.5",
        "@visulima/string": "1.5.1",
        "@vitest/coverage-v8": "^3.2.4",
        "axios": "^1.11.0",
        "cheerio": "^1.1.2",
        "conventional-changelog-conventionalcommits": "9.1.0",
        "eslint": "^9.33.0",
        "eslint-plugin-zod": "^1.4.0",
        "prettier": "3.6.2",
        "puppeteer": "^24.16.2",
        "rss-parser": "3.13.0",
        "secretlint": "11.0.2",
        "semantic-release": "^24.2.7",
        "semantic-release-plugin-update-version-in-files": "^2.0.0",
        "sort-package-json": "^3.4.0",
        "tsx": "^4.20.6",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.39.1",
        "vitest": "^3.2.4"
    },
    "engines": {
        "node": ">=20"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    }
}
