{
  "name": "@anolilab/provider-registry",
  "description": "Unified registry for AI model providers and their metadata",
  "version": "0.0.0",
  "homepage": "https://github.com/anolilab/ai-models",
  "bugs": {
    "url": "https://github.com/anolilab/ai-models/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anolilab/ai-models.git",
    "directory": "packages/provider-registry"
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/prisis"
    },
    {
      "type": "consulting",
      "url": "https://anolilab.com/support"
    }
  ],
  "license": "Apache-2.0",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./schema": {
      "import": "./dist/schema.js",
      "types": "./dist/schema.d.ts"
    },
    "./icons/*": "./dist/icons/*"
  },
  "files": [
    "./dist"
  ],
  "keywords": [
    "ai",
    "models",
    "providers",
    "registry",
    "metadata"
  ],
  "author": "AnoliLab",
  "scripts": {
    "aggregate": "tsx ./scripts/aggregate-providers.ts",
    "build": "pnpm run aggregate && packem build --development",
    "build:prod": "pnpm run aggregate && packem build --production",
    "download": "node ./scripts/download/index.ts",
    "install:chrome": "npx puppeteer browsers install chrome",
    "clean": "rimraf node_modules dist .eslintcache",
    "lint:attw": "attw --pack",
    "lint:eslint": "eslint .",
    "lint:eslint:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "test": "vitest run --typecheck",
    "test:coverage": "vitest run --coverage --coverage.reporter=text"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/node": "^24.1.0",
    "@visulima/packem": "2.0.0-alpha.4",
    "axios": "^1.11.0",
    "cheerio": "^1.1.2",
    "eslint": "^9.32.0",
    "prettier": "3.6.2",
    "puppeteer": "^24.15.0",
    "rss-parser": "3.13.0",
    "semantic-release": "^24.2.7",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vitest": "^3.2.4",
    "@visulima/string": "1.5.1"
  },
  "dependencies": {
    "zod": "^4.0.14"
  },
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}