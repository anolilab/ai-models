{
  "name": "ai-models",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*",
    "tools/*"
  ],
  "scripts": {
    "dev": "pnpm -r dev",
    "build": "pnpm -r build",
    "check-types": "pnpm -r check-types",
    "dev:native": "nx run native:dev",
    "dev:web": "nx run web:dev",
    "dev:server": "nx run server:dev",
    "nx": "nx",
    "nx:graph": "nx graph"
  },
  "nx": {
    "affected": {
      "defaultBase": "main"
    }
  },
  "packageManager": "pnpm@10.8.0",
  "devDependencies": {
    "nx": "16.0.0"
  }
}
